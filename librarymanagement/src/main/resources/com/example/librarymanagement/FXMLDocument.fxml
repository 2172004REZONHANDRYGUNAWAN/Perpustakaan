                  <Image url="@GAMBAR/LOGIN.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Button fx:id="close" layoutX="631.0" layoutY="10.0" mnemonicParsing="false" onAction="#exit" prefHeight="26.0" prefWidth="46.0" styleClass="close" text="X" textFill="WHITE" />
