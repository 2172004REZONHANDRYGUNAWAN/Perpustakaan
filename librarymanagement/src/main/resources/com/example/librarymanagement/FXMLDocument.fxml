<AnchorPane prefHeight="469.0" prefWidth="691.0" styleClass="login-form" stylesheets="@../../../../CSS/desainLogin.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagement.FXMLDocumentController">
   <children>
      <AnchorPane layoutX="123.0" layoutY="68.0" prefHeight="344.0" prefWidth="487.0" styleClass="card-form" stylesheets="@../../../../CSS/desainLogin.css">
         <children>
            <TextField fx:id="studentNumber" layoutX="110.0" layoutY="155.0" prefHeight="34.0" prefWidth="268.0" promptText="Student Number" styleClass="textfield" stylesheets="@../../../../CSS/desainLogin.css" />
            <PasswordField fx:id="password" layoutX="110.0" layoutY="202.0" prefHeight="34.0" prefWidth="270.0" promptText="Password" styleClass="textfield" stylesheets="@../../../../CSS/desainLogin.css" />
